{
    
    "files.exclude": {
        "node_modules": true,
        ".vscode": true,
        ".gitignore": true,
        "package.json": false,
        "package-look.json": true,
        "yarn.lock": true,
        "public": true,
        "README.md": true,
        ".expo": true,
        ".expo-shared": true,
        ".github": true,
        "assets": true,
        "tsconfig.json": true,
        "babel.config.js": true,
        ".next": true
    },

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true
    }
}
